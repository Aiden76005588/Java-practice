
public class Cat {

	String gender;	//유일속성
	public Cat(String gender) {
		super();
		this.gender = gender;
	}
	
	//동작
	
	public String printInfo() {
		return gender;
	}
	public void cryCat() {
		System.out.println("야옹~");
	}
	public void eatCat() {
		System.out.println("생선 먹기");
	}
	public void sleepCat() {
		System.out.println("새근새근~");
	}
		


	public String getGender() {
		return gender;
	}
	public void setGender(String gender) {
		this.gender = gender;
	}
	
	
}
