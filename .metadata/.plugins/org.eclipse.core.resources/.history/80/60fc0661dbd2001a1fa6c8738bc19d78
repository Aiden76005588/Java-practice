
public class ArrayTest01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sum =0;
		int avg =0;
		int i;
		for(i=0; i<=50; i++) {
			if(i%10 ==0) {
				sum+=i;
				if(i!=i) System.out.print("+");
				System.out.print(i);
			}
			
		}
		System.out.print("="+sum);
	}

}
