
public class Pet {

	 String name;
	 int age;
	 public Pet() {System.out.println("Pet기본생성자 호출=====")};
	 
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	 
	 
	 
}
