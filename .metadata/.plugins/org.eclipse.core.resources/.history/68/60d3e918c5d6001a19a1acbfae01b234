package Poly_test;


public class Emp extends Employee {

	String skill;

	public Emp() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Emp(String ssn, String name, String salary, String skill) {
		super(ssn, name, salary);
		this.skill = skill;
	}

	@Override
	public String getEmployee() {
		// TODO Auto-generated method stub
		return super.getEmployee()+"\t"+skill;
	}
	
	
}
