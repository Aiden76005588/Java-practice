package Poly_test;
public class Man extends Emp {

	String depart; //관리부서

	public Man() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Man(String name, String depart) {
		super(name);
		this.depart = depart;
	}
	@Override
	public String printInfo() {
		
		return super.printInfo()+"\t"+depart;
		//return name+"\t"+depart;
	}
	
}
