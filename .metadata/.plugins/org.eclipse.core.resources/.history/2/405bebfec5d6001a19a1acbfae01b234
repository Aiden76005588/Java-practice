package Poly_test;
public class Man extends Emp {

	String depart; //관리부서

	public Man() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Man(String ssn, String name, String salary, String depart) {
		super(ssn, name, salary);
		this.depart = depart;
	}
	@Override
	public String getEmployee() {
		
		return super.getEmployee()+"\t"+depart;
		//return ssn+"\t"+name+"\t"+salary+"\t"+depart;
	}
	
}
